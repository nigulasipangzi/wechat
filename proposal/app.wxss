.tab {
  height: 80rpx;
  text-align: center;
  line-height: 60rpx;
  box-sizing: border-box;
  padding-top: 10rpx;
}

.tab-focus {
  color: #09bb07;
  border-bottom: #09bb07 solid 6rpx;
}

.tab-blur {
  color: #000;
}

input {
  width: 100%;
  height: 60rpx;
  box-sizing: border-box;
  padding: 0 10rpx 0 10rpx;
  border-bottom: #dddddd solid 1px;
  font-size: 1rem;
}

.search {
  width: 710rpx;
  height: 60rpx;
  background-color: #fff;
  border-radius: 6rpx;
  font-size: 1rem;
  display: flex;
}

.search input {
  width: 550rpx;
  height: 60rpx;
  border: 0;
  background: transparent;
  font-size: 1rem;
}

.search image {
  opacity: 0.5;
  width: 48rpx;
  height: 48rpx;
  margin: 6rpx 16rpx 6rpx 16rpx;
}

.eclipse {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.div {
  display: flex;
  flex-direction: column;
}

