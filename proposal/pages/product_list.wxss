.popwin {
  background-color: #fff;
  height: 100%;
}

.console {
  width: 100%;
  padding: 20rpx;
  background-color: #ddd;
}

.keys {
  box-sizing: border-box;
  width: 100%; 
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx;
  border-bottom: #ddd solid 1px;
  background-color: #fff;
}

.list-item {
  height: 100rpx;
  background-color: #fff;
  border-bottom: #ddd solid 1px;
  display: flex;
}

.list-item-icon {
  width: 80rpx;
  height: 80rpx;
  margin: 10rpx 10rpx 10rpx 20rpx;
}

.list-item-icon image {
  width: 80rpx;
  height: 80rpx;
}

.list-item-content {
  width: 530rpx;
  padding: 10rpx 20rpx 10rpx 10rpx;
}

.search-key {
  padding: 0 20rpx 0 20rpx;
  line-height: 60rpx;
}